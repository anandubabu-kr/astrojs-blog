---
import BaseLayout from 'layouts/BaseLayout.astro'
const allPosts = await Astro.glob('./content/react/*.md');
const pageTitle = "Topics from react js";
---

<BaseLayout pageTitle={pageTitle}>
  <p></p>
  <ul class='blog-links'>
    {allPosts.map((post) => (
      <li class='blog-link'>
        <a class='blog-link' href={post.url}>{post.frontmatter.title}</a>
      </li>
    ))}
  </ul>
</BaseLayout>
